<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
  <Windows>
    <Window name="CenterScreenText" movable="false">

      <EventHandlers>
        <EventHandler event="OnUpdate" function="EnhancePack.Update"/>
      </EventHandlers>

      <Windows>

        <Label name="CenterScreenLabel" font="font_verdana_BIG" textalign="center"   handleinput="false" wordwrap="true" linespacing="18">
          <Anchors>
            <Anchor point="center" relativePoint="center">
              <AbsPoint x="900" y="450" />
            </Anchor>
          </Anchors>
          <Color r="255" g="255" b="255" a="255" />
        </Label>
        <DynamicImage name="TextureText" texture="CenterStaticText" layer="overlay">
          
          <Anchors>
            <Anchor point="center" relativePoint="center">
              <AbsPoint x="200" y="100" />
            </Anchor>
          </Anchors>
        </DynamicImage>
      </Windows>
    </Window>
  </Windows>
</Interface>